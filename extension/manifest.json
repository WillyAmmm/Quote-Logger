{
  "manifest_version": 3,
  "name": "Quote Logger",
  "description": "Capture Blue Yonder spot bids and sync to QuoteLog.",
  "version": "1.0.3",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "64": "icons/icon-64.png",
    "128": "icons/icon-128.png",
    "256": "icons/icon-256.png",
    "512": "icons/icon-512.png"
  },
  "action": {
    "default_title": "Quote Logger",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon-16.png",
      "19": "icons/icon-19.png",
      "24": "icons/icon-24.png",
      "32": "icons/icon-32.png",
      "38": "icons/icon-38.png"
    }
  },
  "permissions": [
    "storage",
    "scripting",
    "activeTab",
    "notifications",
    "windows"
  ],
  "host_permissions": [
    "https://script.google.com/*",
    "*://tbc-aztms-pr2.jdadelivers.com/*",
    "*://*.jdadelivers.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://tbc-aztms-pr2.jdadelivers.com/*",
        "*://*.jdadelivers.com/*"
      ],
      "js": ["content.js"],
      "run_at": "document_idle",
      "all_frames": true
    }
  ]
}
